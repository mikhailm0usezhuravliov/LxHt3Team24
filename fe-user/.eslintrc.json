{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:prettier/recommended",
      "prettier",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "prettier",
      "import",
      "@typescript-eslint"
  ],
  "rules": {
      "no-debugger": "off",
      "no-console": 0,
      "class-methods-use-this": "off",
      "@typescript-eslint/no-explicit-any": "warn",
      "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
